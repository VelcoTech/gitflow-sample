<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <activeProfiles>
        <activeProfile>velco-tech</activeProfile>
    </activeProfiles>
    <profiles>
        <profile>
            <id>velco-tech</id>
            <repositories>
                <repository>
                    <id>velco-framework-back</id>
                    <url>https://maven.pkg.github.com/VelcoTech/velco-framework-back</url>
                </repository>
                <repository>
                    <id>google</id>
                    <url>https://maven.google.com</url>
                </repository>
            </repositories>
        </profile>
    </profiles>
    <servers>
        <server>
            <id>velco-framework-back</id>
            <username>github</username>
            <password>${env.GITHUB_TOKEN}</password>
        </server>
    </servers>
</settings>
